package poo.director;

public class Pelicula {

	private String titulo, director;
	private int anno_creacion;
	
	public Pelicula(String titulo, int anno_creacion) {
		this.titulo = titulo;
		this.anno_creacion = anno_creacion;
	}

	public Pelicula(String director) {
		this.director = director;
	}

	public Pelicula() {
		super();
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	public String getDirector() {
		return director;
	}

	public void setDirector(String director) {
		this.director = director;
	}

	public int getAnno_creacion() {
		return anno_creacion;
	}

	public void setAnno_creacion(int anno_creacion) {
		this.anno_creacion = anno_creacion;
	}
	
	public String toStringPelicula() {
		String s;
		s=("||||Pelicula|||| \n");
		s+=("Titulo=" + titulo + "\n");
		s+=("AÃ±o de estreno= " + anno_creacion + "\n");
		s+=("Director= " + director + "\n");
		return s;

	}
}
